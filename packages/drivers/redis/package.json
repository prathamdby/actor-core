{
  "name": "@actor-core/redis",
  "version": "0.3.0",
  "files": [
    "src",
    "dist",
    "package.json"
  ],
  "type": "module",
  "exports": {
    "./actor.ts": {
      "import": {
        "types": "./dist/actor.d.ts",
        "default": "./dist/actor.js"
      },
      "require": {
        "types": "./dist/actor.d.cts",
        "default": "./dist/actor.cjs"
      }
    },
    "./manager.ts": {
      "import": {
        "types": "./dist/manager.d.ts",
        "default": "./dist/manager.js"
      },
      "require": {
        "types": "./dist/manager.d.cts",
        "default": "./dist/manager.cjs"
      }
    },
    "./p2p.ts": {
      "import": {
        "types": "./dist/p2p.d.ts",
        "default": "./dist/p2p.js"
      },
      "require": {
        "types": "./dist/p2p.d.cts",
        "default": "./dist/p2p.cjs"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/actor.ts src/manager.ts src/p2p.ts",
    "check-types": "tsc --noEmit"
  },
  "peerDependencies": {
    "actor-core": "workspace:*"
  },
  "devDependencies": {
    "actor-core": "workspace:*",
    "tsup": "^8.3.6",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@types/node": "^22.13.1",
    "dedent": "^1.5.3",
    "hono": "^4.7.0",
    "ioredis": "^5.4.2",
    "p-retry": "^6.2.1"
  }
}
